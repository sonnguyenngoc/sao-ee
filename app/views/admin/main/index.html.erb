<!-- Main navbar -->
<%= render "main_navbar" %>
<!-- /main navbar -->

<!-- Page container -->
<div class="page-container">

    <!-- Page content -->
    <div class="page-content">

        <!-- Main sidebar -->
        <div class="sidebar sidebar-main">
            <div class="sidebar-content">

                <!-- User menu -->
                <%= render "user_menu" %>
                <!-- /user menu -->

                <!-- Main navigation -->
                <%= render "main_menu" %>
                <!-- /main navigation -->

            </div>
        </div>
        <!-- /main sidebar -->

        <!-- Main content -->
        <div class="content-wrapper">

            <!-- Page header -->
            <div class="page-header">
                <div class="page-header-content">
                    <div class="page-title">
                        <h4><span class="text-semibold"><%= t(:home) %> - <%= t(:dashboard) %></span></h4>
                    </div>
                </div>

                <div class="breadcrumb-line">
                    <ul class="breadcrumb">
                        <li><%= link_to '<i class="icon-home2 position-left"></i>'.html_safe + t(:home), { controller: "main", action: "index" } %></li>
                        <li class="active"><%= t(:dashboard) %></li>
                    </ul>
                </div>
            </div>
            <!-- /page header -->

            <!-- Content area -->
            <div class="content">
                
                <!-- Dashboard content -->
                <div class="row">
                    <div class="col-lg-12">
        
                        <h1 class="text-center text-teal"><%= t(:welcome) %></h1>
        
                    </div>
                </div>
                <!-- /dashboard content -->

            </div>
            <!-- /content area -->

        </div>
        <!-- /main content -->

    </div>
    <!-- /page content -->

</div>
<!-- /page container -->

<!-- Theme JS files -->
<%= javascript_include_tag "/backend/assets/js/plugins/visualization/d3/d3.min.js" %>
<%= javascript_include_tag "/backend/assets/js/plugins/visualization/d3/d3_tooltip.js" %>
<%= javascript_include_tag "/backend/assets/js/plugins/forms/styling/switchery.min.js" %>
<%= javascript_include_tag "/backend/assets/js/plugins/forms/styling/uniform.min.js" %>
<%= javascript_include_tag "/backend/assets/js/plugins/forms/selects/bootstrap_multiselect.js" %>
<%= javascript_include_tag "/backend/assets/js/plugins/ui/moment/moment.min.js" %>
<%= javascript_include_tag "/backend/assets/js/plugins/pickers/daterangepicker.js" %>

<%= javascript_include_tag "/backend/assets/js/core/app.js" %>
<%= javascript_include_tag "/backend/assets/js/pages/dashboard.js" %>
<!-- /theme JS files -->